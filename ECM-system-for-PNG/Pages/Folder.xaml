<Page x:Class="ECM_system_for_PNG.Pages.Folder"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ECM_system_for_PNG.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Folder"
      KeepAlive="True">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Button x:Name="buttonBack" Content="Назад" Click="buttonBack_Click" Margin="10"/>
        <ListBox x:Name="listBox" Grid.Row="1" Margin="10" MouseDoubleClick="listBox_MouseDoubleClick"/>
    </Grid>

</Page>
