<ResourceDictionary 
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <FontFamily x:Key="BoldBaseFont">Space Grotesk</FontFamily>
    <FontFamily x:Key="LightBaseFont">Space Grotesk Light</FontFamily>
    <system:Double x:Key="Smal">14</system:Double>
    <system:Double x:Key="Regular">20</system:Double>
    <system:Double x:Key="Big">40</system:Double>

</ResourceDictionary>