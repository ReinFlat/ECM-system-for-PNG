<Application x:Class="ECM_system_for_PNG.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:ECM_system_for_PNG"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <DrawingImage x:Key="EmptyImage"/>
            <local:PageToEnabledConverter x:Key="PageToEnabledConverter"/>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Styles\Colors.xaml" />
                <ResourceDictionary Source="Styles\Fonts.xaml" />
                <ResourceDictionary Source="Styles\Icons.xaml" />
                
                <ResourceDictionary Source="Styles\Texts.xaml" />
                <ResourceDictionary Source="Styles\Buttons.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
